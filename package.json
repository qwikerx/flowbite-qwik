{
  "name": "flowbite-qwik",
  "version": "1.0.0",
  "description": "Create a reusable Qwik component library implementing Flowbite",
  "module": "./lib/index.js",
  "main": "./lib/index.umd.cjs",
  "types": "./lib-types/index.d.ts",
  "type": "module",
  "style": "./lib/style.css",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.umd.cjs",
      "types": "./lib-types/index.d.ts"
    },
    "./package.json": "./package.json",
    "./lib/*": "./lib/*"
  },
  "files": [
    "lib",
    "lib-types"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "scripts": {
    "build": "rm -rf lib && rm -rf lib-types && vite build --mode lib && pnpm run build.types",
    "build.watch": "vite build --mode lib --watch",
    "build.types": "tsc --emitDeclarationOnly --declaration",
    "build.types.watch": "tsc --watch --emitDeclarationOnly --declaration",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "np",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "dependencies": {
    "@qwikest/icons": "^0.0.13",
    "class-variance-authority": "^0.7.0"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.5.4",
    "@builder.io/qwik-city": "1.5.4",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-qwik": "1.5.4",
    "np": "^10.0.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "undici": "^6.16.1",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  }
}
